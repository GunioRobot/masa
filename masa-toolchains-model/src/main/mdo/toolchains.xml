<?xml version="1.0"?>

<model xmlns="http://modello.codehaus.org/MODELLO/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xml.namespace="http://jvending.org/toolchains/1.0.0">
	<id>masa-toolchains</id>
	<name>MasaToolchains</name>
	<defaults>
		<default>
			<key>package</key>
			<value>org.jvending.masa.toolchains.model</value>
		</default>
	</defaults>
	<classes>
		<class rootElement="true" xml.tagName="toolchains">
			<name>Toolchains</name>
			<version>1.0.0+</version>
			<fields>
				<field>
					<name>toolchain</name>
					<version>1.0.0+</version>
					<association xml.mapStyle="inline">
						<type>Toolchain</type>
						<multiplicity>*</multiplicity>
					</association>
				</field>
			</fields>
		</class>
		<class>
			<name>Toolchain</name>
			<version>1.0.0+</version>
			<fields>
				<field>
					<name>id</name>
					<version>1.0.0+</version>
					<type>String</type>
				</field>
				<field>
					<name>toolPaths</name>
					<version>1.0.0+</version>
					<association xml.mapStyle="inline">
						<type>ToolPaths</type>
						<multiplicity>*</multiplicity>
					</association>
				</field>
			</fields>
		</class>		
		<class>
			<name>ToolPaths</name>
			<version>1.0.0+</version>
			<fields>
				<field>
					<name>toolPath</name>
					<version>1.0.0+</version>
					<type>String</type>
				</field>
			</fields>
		</class>					
	</classes>
</model>
